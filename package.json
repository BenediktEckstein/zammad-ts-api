{
  "name": "zammad-ts-api",
  "version": "1.0.8",
  "description": "Zammad API implementation for TS",
  "main": "dist/index.js",
  "type":"module",
  "scripts": {
    "compile": "NODE_OPTIONS='--max-old-space-size=4096' tsc",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "build": "npm run compile && npm run test",
    "coveralls": "npm run test --coverage && cat ./coverage/lcov.info | coveralls",
    "doc": "jsdoc -c .jsdoc.conf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BryceDalbey/zammad-ts-api.git"
  },
  "keywords": [
    "zammad",
    "ticket",
    "helpdesk"
  ],
  "author": "Bryce Dalbey",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/BryceDalbey/zammad-ts-api/issues"
  },
  "homepage": "https://github.com/BryceDalbey/zammad-ts-api#readme",
  "dependencies": {
    "axios": "^1.7.2",
    "axios-debug-log": "^0.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "coveralls": "^3.0.11",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "jest": "^29.7.0",
    "jsdoc": "^3.6.4",
    "randomstring": "^1.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "files": [
    "./dist"
  ]
}
